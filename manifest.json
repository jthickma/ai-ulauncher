{
    "required_api_version": "^2.0.0",
    "name": "Answering AI Pro",
    "description": "AI Assistant with OpenRouter, custom presets, and automatic Markdown logging.",
    "developer_name": "nktauserum",
    "icon": "images/icon.png",
    "options": {
        "query_debounce": 1
    },
    "preferences": [
        {
            "id": "gpt_kw",
            "type": "keyword",
            "name": "AI Prompt",
            "default_value": "ais"
        },
        {
            "id": "model_kw",
            "type": "keyword",
            "name": "Model Finder",
            "default_value": "ais-m",
            "description": "Keyword to search and fetch available OpenRouter models."
        },
        {
            "id": "preset_kw",
            "type": "keyword",
            "name": "Preset Selector",
            "default_value": "ais-p",
            "description": "Keyword to switch between your custom system prompt presets."
        },
        {
            "id": "api_key",
            "type": "input",
            "name": "Openrouter API Key",
            "default_value": ""
        },
        {
            "id": "model",
            "type": "input",
            "name": "Active Model",
            "default_value": "google/gemini-2.0-flash-exp:free",
            "description": "The current model ID. Use ais-m to find others."
        },
        {
            "id": "custom_presets",
            "type": "input",
            "name": "Prompt Presets (JSON)",
            "description": "Format: {\"Coding\": \"Expert dev\", \"Writing\": \"Pro editor\"}",
            "default_value": "{}"
        },
        {
            "id": "log_dir",
            "type": "input",
            "name": "Logs Directory",
            "description": "Absolute path for logs. Defaults to Home (~/) if empty.",
            "default_value": ""
        },
        {
            "id": "system_prompt",
            "type": "input",
            "name": "Default System Prompt",
            "default_value": "You are a helpful assistant."
        },
        {
            "id": "temperature",
            "type": "input",
            "name": "Temperature",
            "default_value": "0.7"
        }
    ]
}